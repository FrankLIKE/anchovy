			"derelict:glfw3" : "~master",
			"derelict:opengl3" : "~master",
			"derelict:util" : "~master",
			"dlib" : "~master"
		},
		"importPaths" : [ "import" ],
		"sourcePaths" : [ "import/anchovy/gui" ],
